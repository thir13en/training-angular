{
  "name": "angular-training",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:proxy": "ng serve --proxy-config proxy.conf.json",
    "install:fresh": "rm -rf node_modules npm-shrinkwrap.json && npm install && npm shrinkwrap && npm run start",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "ng test",
    "test:single": "ng test --watch=false",
    "lint": "ng lint --format=stylish",
    "lint:fix": "ng lint --fix",
    "check": "ng build && ng build --prod && ng lint && npm run test:single",
    "e2e": "ng e2e",
    "dev:ssr": "ng run angular-training:serve-ssr",
    "serve:ssr": "node dist-server/main.js",
    "build:ssr": "ng build --prod && ng run angular-training:server:production",
    "prerender": "ng run angular-training:prerender"
  },
  "dependencies": {
    "@angular/animations": "~11.0.5",
    "@angular/cdk": "~11.0.3",
    "@angular/common": "~11.0.5",
    "@angular/compiler": "~11.0.5",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~11.0.5",
    "@angular/material": "~11.0.3",
    "@angular/platform-browser": "~11.0.5",
    "@angular/platform-browser-dynamic": "~11.0.5",
    "@angular/platform-server": "~11.0.5",
    "@angular/router": "~11.0.5",
    "normalize.css": "~8.0.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.5",
    "@angular-eslint/builder": "0.8.0-beta.3",
    "@angular-eslint/eslint-plugin": "0.8.0-beta.3",
    "@angular-eslint/eslint-plugin-template": "0.8.0-beta.3",
    "@angular-eslint/schematics": "~0.8.0-beta.6",
    "@angular-eslint/template-parser": "0.8.0-beta.3",
    "@angular/cli": "~11.0.5",
    "@angular/compiler-cli": "~11.0.5",
    "@nguniversal/builders": "~11.0.0",
    "@nguniversal/express-engine": "~11.0.0",
    "@types/express": "~4.17.8",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.0",
    "@types/node": "~14.14.14",
    "@types/resize-observer-browser": "~0.1.4",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "codelyzer": "^6.0.0",
    "eslint": "~7.16.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "husky": "~4.2.5",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.14",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "typescript": "~4.0.5"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run check"
    }
  }
}
